{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Driver",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args":[
                "-v", "--run", "Regress", "--args", "n=999999,p=0.01;5.0;1.0;2.0;0.1"
            ],
            "stopAtEntry": false,
            "cwd":"${workspaceFolder}",
            "MIMode": "gdb",
        }
    ]
}